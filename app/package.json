{
  "name": "rnn-starter",
  "version": "11.2.0",
  "description": "Production-ready starter for your next React Native App! Powered by cli-rn, React Native Navigation, RN UI lib, Mobx, Reanimated 2, Notifications, Permissions, Dark Mode, Localization, and much more.",
  "author": "Batyr <dev@batyr.io>",
  "homepage": "https://github.com/kanzitelli/rnn-starter",
  "private": false,
  "scripts": {
    "android": "yarn android:adb && npx react-native run-android",
    "android:release": "yarn android --variant release",
    "android:adb": "adb reverse tcp:8081 tcp:8081",
    "ios": "npx react-native run-ios",
    "ios:release": "yarn ios --configuration Release",
    "ios:pods": "npx pod-install",
    "ios:pods:ru": "cd ios && pod install --repo-update && cd ..",
    "start": "npx react-native start",
    "rename": "npx react-native-rename",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --check ./src",
    "format:write": "prettier --write ./src",
    "release": "dotenv release-it",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@kanzitelli/if-component": "^0.2.1",
    "@react-native-async-storage/async-storage": "^1.15.9",
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/datetimepicker": "^3.5.2",
    "@react-native-community/netinfo": "^6.0.4",
    "@react-native-picker/picker": "^2.1.0",
    "date-fns": "^2.25.0",
    "formik": "^2.2.9",
    "i18n-js": "^3.8.0",
    "lodash": "^4.17.21",
    "mobx": "^6.3.5",
    "mobx-persist-store": "^1.0.4",
    "mobx-react": "^7.2.1",
    "react": "^17.0.2",
    "react-native": "^0.66.1",
    "react-native-device-info": "^8.4.3",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-localize": "^2.1.5",
    "react-native-mmkv": "^1.5.3",
    "react-native-navigation": "^7.23.1",
    "react-native-navigation-hooks": "^6.3.0",
    "react-native-reanimated": "^2.2.3",
    "react-native-restart": "^0.0.22",
    "react-native-ui-lib": "^6.2.0",
    "react-native-vector-icons": "^9.0.0",
    "rn-bounceable": "^0.2.0",
    "rnn-screens": "^0.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.15.4",
    "@react-native-community/eslint-config": "^3.0.1",
    "@tsconfig/react-native": "^1.0.4",
    "@types/i18n-js": "^3.8.2",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/react": "^17.0.33",
    "@types/react-native": "^0.66.1",
    "@types/react-native-vector-icons": "^6.4.9",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/parser": "^5.2.0",
    "babel-jest": "^27.3.1",
    "dotenv-cli": "^4.0.0",
    "eslint": "^8.1.0",
    "jest": "^27.3.1",
    "metro-react-native-babel-preset": "^0.66.2",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.4.1",
    "react-test-renderer": "^17.0.2",
    "release-it": "^14.11.6",
    "typescript": "^4.4.4"
  },
  "keywords": [
    "react",
    "react-native",
    "react-native-navigation",
    "react-native-starter"
  ],
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "license": "MIT"
}
